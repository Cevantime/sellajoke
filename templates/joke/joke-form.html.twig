{% extends "layout.html.twig" %}

{% block title "Ajout d'une blague" %} 
{% block content %}
	{% form_theme form "bootstrap_3_layout.html.twig" %}
	<!-- Header -->
	<header>
		<div class="container" id="maincontent" tabindex="-1">
			<div class="row">
				<div class="col-lg-12">
					
					<div class="intro-text">
						<h1 class="name">Ajout d'une blague</h1>
						<hr class="star-light">
						
					</div>
				</div>
			</div>
		</div>
	</header>
	
	{{ dump(form) }}
	<div class="container">
		<div class="row">
			<div class="col-md-6 col-md-offset-3">
				<h2 class="text-center">Ajouter une blague</h2>
				{{ form(form) }}
			</div>
		</div>
	</div>
{% endblock %}
